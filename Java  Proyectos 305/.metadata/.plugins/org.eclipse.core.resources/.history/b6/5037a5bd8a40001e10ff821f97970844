import java.util.Scanner;
public class Hour {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int hr, mn;
		String[]he= {" twelve ", " one ", " two ", " three ", " four ", " five ", " six ", " seven ", " eight ", 
				" nine "," ten ", " eleven "};
		String[]me= {" o'clock ", " half past ", " quarter past ", " quarter to ", " past ", " to "};	

		
		System.out.println("Introduce hora (0 a 12)");
		hr=sc.nextInt();
		System.out.println("Introduce minutos (0 a 59)");
		mn=sc.nextInt();
		
		switch(hr) {
		case 1:
			System.out.println("It's " + he[0]);
		if(mn==0) {
			System.out.println(me[0]);
		}
		}
	}

}
