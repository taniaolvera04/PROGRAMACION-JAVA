import java.util.Scanner;
public class HrEnglish {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int hr, mn;
		int[][]banco=new int[23][59];
		System.out.println("Introduce hora de 0 a 23");
		hr=sc.nextInt();
		System.out.println("Introduce minutos de 0 a 59");
		mn=sc.nextInt();
		if(hr==0 && mn==0) {
			System.out.println("It's twelve o'clock");
		}
	}

}
