import javax.swing.JOptionPane;

public class Contrase単aSwitch {
	public static void main(String[] args) {
		String passw1="ILOVEBTS";
		String passw2=" ";
		passw2=JOptionPane.showInputDialog("Introduce tu contrase単a de acceso");
		switch (passw1) {
		case "1":
			if (passw1.equals(passw2)==true) {
				JOptionPane.showMessageDialog(null, "Acceso permitido");
			}
		case "2":
			 if(passw1.equals(passw2)==false){
				JOptionPane.showInputDialog("Contrase単a incorrecta, introduce la contrase単a correcta");
			}
		} JOptionPane.showMessageDialog(null, "Acceso denegado");
	}

}
