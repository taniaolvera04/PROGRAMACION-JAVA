import javax.swing.JOptionPane;
public class Factorial {
	public static void main(String[] args) {
		int num, resul;
		num=Integer.parseInt(JOptionPane.showInputDialog("Numero"));
		for (int i=0; i==0; i--) {
			resul=(num*i--);
			JOptionPane.showMessageDialog(null, "Factorial es " + resul);
		}
	}
}
