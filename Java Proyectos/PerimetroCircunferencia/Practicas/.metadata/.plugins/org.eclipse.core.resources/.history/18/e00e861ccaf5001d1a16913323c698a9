import javax.swing.*;
public class Array {
	public static void main(String[] args) {
		int[]array=new int[5];
				array[0]=5;
				array[1]=38;
				array[2]=-15;
				array[3]=92;
				array[4]=71;
				JOptionPane.showMessageDialog(null, "El número que está en la posición 3 del array es: " + array[3]);
	}

}
